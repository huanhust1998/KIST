<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <welcome-file-list>
        <welcome-file>view/index.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>studentController</servlet-name>
        <servlet-class>controller.AddStudentController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>studentController</servlet-name>
        <url-pattern>/addStudents</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>subjectController</servlet-name>
        <servlet-class>controller.AddSubjectController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>subjectController</servlet-name>
        <url-pattern>/addSubjects</url-pattern>
    </servlet-mapping>
</web-app>